[user]
  name = <%= @user_name %>
  email = <%= @user_email %>
[core]
  excludesfile = <%= @current_home %>/.gitignore_global
  editor = <%= @editor %>
[difftool "sourcetree"]
  cmd = opendiff \"$LOCAL\" \"$REMOTE\"
  path =
[mergetool "sourcetree"]
  cmd = /Applications/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
  trustExitCode = true
[color]
  ui = true
[alias]
<%- @alias.each do |name, value| -%>
  <%= name %> = <%= value %>
<%- end -%>
